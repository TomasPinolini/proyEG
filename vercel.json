{
    "version": 2,
    "builds": [
      {
        "src": "*.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      { "src": "/login", "dest": "/login.php" },
      { "src": "/register", "dest": "/register.php" },
      { "src": "/activation_account", "dest": "/activation_account.php" },
      { "src": "/db", "dest": "/db.php" },
      { "src": "/logout", "dest": "/logout.php" },
      { "src": "/menuAdmin", "dest": "/menuAdmin.php" },
      { "src": "/menuCliente", "dest": "/menuCliente.php" },
      { "src": "/menuDueno", "dest": "/menuDueno.php" },
      { "src": "/todas_promociones", "dest": "/todas_promociones.php" },
      { "src": "/ver_novedades", "dest": "/ver_novedades.php" },

      { "src": "/menuAdmin/aprobar_duenos", "dest": "/menuAdmin/aprobar_duenos.php" },
      { "src": "/menuAdmin/aprobar_promos", "dest": "/menuAdmin/aprobar_promos.php" },
      { "src": "/menuAdmin/crear_locales", "dest": "/menuAdmin/crear_locales.php" },
      { "src": "/menuAdmin/novedades", "dest": "/menuAdmin/novedades.php" },
      { "src": "/menuAdmin/uso_desc", "dest": "/menuAdmin/uso_desc.php" },

      { "src": "/menuDueno/gestion_descuentos", "dest": "gestion_descuentos/menuDueno/.php" },
      { "src": "/menuDueno/ingresar_promo", "dest": "ingresar_promo/menuDueno/.php" },
      { "src": "/menuDueno/uso_promos", "dest": "uso_promos/menuDueno/.php" },

      { "src": "/menuCliente/usar_promo", "dest": "/menuCliente/usar_promo.php" },
      { "src": "/menuCliente/ver_novedades", "dest": "/menuCliente/ver_novedades.php" },
      { "src": "/menuCliente/ver_promos", "dest": "/menuCliente/ver_promos.php" },

      { "src": "/(.*)", "dest": "/index.php" }
    ]
  }
  